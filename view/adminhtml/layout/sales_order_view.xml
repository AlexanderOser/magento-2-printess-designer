<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="order_items">
            <arguments>
                <argument name="columns" xsi:type="array">
                    <item name="download_column" xsi:type="string" translate="true">Printess</item>
                </argument>
            </arguments>
            <referenceBlock name="default_order_items_renderer">
                <arguments>
                    <argument name="columns" xsi:type="array">
                        <item name="download_column" xsi:type="string" translate="true">col-download</item>
                    </argument>
                </arguments>
            </referenceBlock>
            <block class="Digitalprint\PrintessDesigner\Block\Sales\Items\Column\Download" name="column_download_column" template="Digitalprint_PrintessDesigner::items/column/download.phtml" group="column" />
        </referenceBlock>
    </body>
</page>
